{
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "vue.volar",
        "johnsoncodehk.volar",
        "bradlc.vscode-tailwindcss"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "editor.rulers": [
          100
        ]
      }
    }
  },
  "dockerComposeFile": [
    "../../docker-compose.yml",
    "../docker-compose.yml"
  ],
  "name": "Dev Container for Frontend",
  "remoteUser": "vscode",
  "service": "frontend",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}/frontend"
}
